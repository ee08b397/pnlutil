#!/bin/bash -e

SCRIPT=$(readlink -m $(type -p $0))
SCRIPTDIR=${SCRIPT%/*}      
source $SCRIPTDIR/util.sh

HELP="
${0##*/}: Prints all <var> file paths. 

$queryscript_helpmsg"

queryscript_parseargs "$@"

for case in $cases; do
    if $selectcases; then
        echo $case
    else
        source "$SetUpData"
        printf "%s\n" ${!var}
    fi
done
