#!/bin/bash -e

SCRIPT=$(readlink -m $(type -p $0))
SCRIPTDIR=${SCRIPT%/*}      
source $SCRIPTDIR/util.sh

HELP="Prints progress ('done/total') for <var>.  

$standard_var_help

Usage:

    ${0##*/} <var>

"

# check args and ./SetUpData.sh
[[ $# -lt 1 || $1 == "-h" || $1 == "--help" ]] && usage 1
var=$1

checkset_SetUpData "$var"
checkset_cases
echo $($SCRIPTDIR/completed $var | wc -l)/$(wc -w < $caselist)
